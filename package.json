{
  "name": "elysjs",
  "version": "1.4.20",
  "author": "Ducksquaddd <duck@kwak.tech>",
  "repository": {
    "type": "git",
    "url": "https://github.com/ducksquaddd/elysjs"
  },
  "license": "SEE LICENSE IN LICENSE",
  "publishConfig": {
    "access": "restricted"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "clean": "lerna run clean",
    "build": "lerna run build --stream",
    "lint": "lerna run lint --parallel",
